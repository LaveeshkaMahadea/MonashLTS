@model IEnumerable<MonashLTS.Models.Student>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AlternateOffering)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentIndication)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Alias)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ChineseNatOrRez)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssessedLocation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OnshoreOffshore)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AlternateOffering)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentIndication)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Alias)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ChineseNatOrRez)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AssessedLocation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OnshoreOffshore)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
